{
  "controller": {
    "host": "localhost",
    "ports": {
      "primary": 30001
    },
    "autoLaunch": {
      "disabled": false,
      "version": "5.3.1",
      "stop": false
    }
  },
  "testServer": {
    "host": "autodiscover",
    "port": 24493,
    "defaultExecutionTimeout": 10000,
    "restartThreshold": 30
  },
  "mocks": {
    "include": ["__mocks__/**/*.mock.script"]
  },
  "sources": {
    "global": {
      "scripts": {
        "include": ["src/**/*.script"],
        "exclude": [
          "src/**/templates/**/*.script",
          "src/**/__tests__/**/*.script"
        ]
      }
    }
  }
}
